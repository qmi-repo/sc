{
  "name": "qmi-libs/stardust-connect-client",
  "description": "Client PHP per utilizzare lo Stardust Connect.",
  "type": "library",
  "homepage": "https://gitlab.qmi.it/qmi-libs/stardust-connect-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Michele Sangalli",
      "email": "michele.sangalli@qmi.it"
    }
  ],
  "autoload": {
    "psr-4": {
      "QMILibs\\StardustConnectClient\\": "src/"
    }
  },
  "require": {
    "php": "^5.6||^7.2",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "~6.0",
    "psr/log": "~1.0",
    "symfony/event-dispatcher": ">=2.4,<5",
    "symfony/options-resolver": ">=2.6,<5",
    "kevinrob/guzzle-cache-middleware": "^1.2 || ^2.0",
    "doctrine/cache": "^1.6"
  },
  "require-dev": {
    "monolog/monolog": ">=1.11.0"
  },
  "suggest": {
    "monolog/monolog": "Required if you want to make use of logging features."
  }
}
